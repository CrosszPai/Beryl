{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "type": "module",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "node -r dotenv/config ./concurrently.js",
    "devscript": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve api/_build",
    "start:ex": "node -r dotenv/config ./start.js"
  },
  "dependencies": {
    "@octokit/auth-token": "^2.5.0",
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/vercel": "^1.4.3",
    "@remix-run/serve": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "@types/lodash": "^4.14.182",
    "chart.js": "^3.7.1",
    "classnames": "^2.3.1",
    "dotenv": "^14.3.2",
    "graphql": "^16.3.0",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.2",
    "octokit": "^1.7.1",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-github-btn": "^1.2.2",
    "remix": "^1.4.0",
    "remix-auth": "^3.2.1",
    "remix-auth-github": "^1.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/randomcolor": "^0.5.6",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "daisyui": "^1.25.4",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
